<%- layout('/layouts/layout') %> 
<h2>Current Projects</h2>
<div class="row">
    <% projects.forEach(project => { %>
        <div class="col-lg-2 project-tab">
            <h3><%= project.name %></h3>
            <p><%= project.deadline %></p>
        </div>
    <% }) %> 
</div>
<a href="/projects/new">Create a new Project</a>